@model BlowOutProject.Models.Instrument

@{
    ViewBag.Title = Model.Name;
}
<link rel="stylesheet" type="text/css" href="~/Content/myStyles.css" />
<h2>@Model.Name</h2>
<h3>Click to see pricing:</h3>
<p>@Html.ActionLink("New", "New", "Rentals", new {nameIn = Model.Name}, null)</p>
<p>@Html.ActionLink("Used", "Used", "Rentals", new { nameIn = Model.Name }, null)</p>
<br />

<img src="~/Content/image/@{@Model.Name}.jpg" class="detailPics" />
